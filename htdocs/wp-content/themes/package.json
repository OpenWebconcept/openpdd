  "main": "index.js",
  "scripts": {
    "start": "node cli.js -s",
    "dev": "wp-scripts start --config ./config/frontend.config.js",
    "dev-all": "run-p -l \"dev -- --env theme=owc-formulieren\" \"dev -- --env theme=buren\" \"dev -- --env theme=ggd-hollands-noorden\" \"dev -- --env theme=waddinxveen\" \"dev -- --env theme=hoekschewaard\"",
    "prod": "wp-scripts build --config ./config/frontend.config.js",
    "prod-all": "run-p -l \"prod -- --env theme=owc-formulieren\" \"prod -- --env theme=buren\" \"prod -- --env theme=ggd-hollands-noorden\" \"prod -- --env theme=waddinxveen\" \"prod -- --env theme=hoekschewaard\"",
    "format:js": "wp-scripts format \"./**/assets/js/src/**/*.{js,ts}\"",
    "lint:js": "wp-scripts lint-js \"./**/assets/js/src/**/*.js\"",
    "lint:scss": "stylelint './**/assets/scss/**/*.scss' --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wordpress/babel-plugin-import-jsx-pragma": "^3.1.2",
    "@wordpress/eslint-plugin": "^7.2.0",
    "@wordpress/prettier-config": "^1.1.3",
    "@wordpress/scripts": "^19.2.2",
    "commander": "^8.3.0",
    "inquirer": "^8.2.0",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "npm:wp-prettier@^2.2.1-beta-1",
    "shelljs": "^0.8.5",
    "stylelint": "^14.2.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "bootstrap": "^4.6.1",
    "js-cookie": "^3.0.1"
  }
}