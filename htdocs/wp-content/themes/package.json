{
	"name": "owc-formulieren",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node cli.js -s",
		"dev": "run-p -l \"dev:** -- {@}\" --",
		"dev:editor": "wp-scripts start --config ./config/editor.config.js",
		"dev:frontend": "wp-scripts start --config ./config/frontend.config.js",
		"dev-all": "run-p -l \"dev -- --env theme=owc-formulieren\" \"dev -- --env theme=buren\" \"dev -- --env theme=ggd-hollands-noorden\" \"dev -- --env theme=waddinxveen\" \"dev -- --env theme=hollandskroon\" \"dev -- --env theme=hoekschewaard\" \"dev -- --env theme=sudwestfryslan\"",
		"prod": "run-p -l \"prod:** -- {@}\" --",
		"prod:editor": "wp-scripts build --config ./config/editor.config.js",
		"prod:frontend": "wp-scripts build --config ./config/frontend.config.js",
		"prod-all": "run-p -l \"prod -- --env theme=owc-formulieren\" \"prod -- --env theme=buren\" \"prod -- --env theme=ggd-hollands-noorden\" \"prod -- --env theme=waddinxveen\" \"prod -- --env theme=hollandskroon\" \"prod -- --env theme=hoekschewaard\" \"prod -- --env theme=sudwestfryslan\"",
		"format:js": "wp-scripts format \"./**/assets/js/**/*.js\"",
		"format:scss": "wp-scripts format \"./**/assets/scss/**/*.scss\"",
		"lint:js": "wp-scripts lint-js \"./**/assets/js/**/*.js\"",
		"lint:scss": "stylelint './**/assets/scss/**/*.scss' --fix"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@nl-design-system-unstable/buren-design-tokens": "^1.0.0-alpha.1",
		"@wordpress/babel-plugin-import-jsx-pragma": "^3.2.0",
		"@wordpress/block-editor": "^10.5.0",
		"@wordpress/blocks": "^11.21.0",
		"@wordpress/components": "^22.1.0",
		"@wordpress/data": "^7.6.0",
		"@wordpress/element": "^4.20.0",
		"@wordpress/eslint-plugin": "^7.2.0",
		"@wordpress/i18n": "^4.22.0",
		"@wordpress/notices": "^3.22.0",
		"@wordpress/prettier-config": "^1.4.0",
		"@wordpress/scripts": "^19.2.2",
		"commander": "^8.3.0",
		"copy-webpack-plugin": "^11.0.0",
		"inquirer": "^8.2.5",
		"mini-css-extract-plugin": "^2.7.2",
		"npm-run-all": "^4.1.5",
		"path": "^0.12.7",
		"prettier": "npm:wp-prettier@^2.6.2",
		"shelljs": "^0.8.5",
		"stylelint": "^14.14.1",
		"stylelint-config-idiomatic-order": "^8.1.0",
		"stylelint-config-recommended": "^6.0.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"@fortawesome/fontawesome-pro": "^6.4.2",
		"bootstrap": "^4.6.2",
		"js-cookie": "^3.0.1"
	}
}
