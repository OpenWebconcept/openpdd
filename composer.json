	"require": {
		"bepsvpt/secure-headers": "^7.0",
		"composer/installers": "^1.0",
		"illuminate/support": "^8.83",
		"johnpbloch/wordpress": "6.3.*",
		"johnpbloch/wordpress-core": "6.3.*",
		"koodimonni-language/core-nl_nl": "6.3.*",
		"php-di/php-di": "^6.0",
		"plugin/config-expander": "^2.0",
		"plugin/formio-wordpress": "dev-main",
		"plugin/gravityforms-amp-booking": "^1.3",
		"plugin/gravityforms-csp-fixer": "^0.0.2",
		"plugin/owc-datab": "*",
		"plugin/owc-gravityforms-bag-address": "1.1.*",
		"plugin/owc-gravityforms-digid": "^1.4",
		"plugin/owc-gravityforms-eherkenning": "^1.1",
		"plugin/owc-gravityforms-kvk-prefill": "^1.0",
		"plugin/owc-gravityforms-zaaksysteem": "dev-feat/generate-pdf-of-created-zaak-and-connect",
		"plugin/owc-gravityforms-zaaksysteem-enable-u": "^1.1.0",
		"plugin/owc-shared-aura-session-instance": "^1.0.0",
		"plugin/prefill-gravity-forms": "^1.2.0",
		"plugin/signicat-openid": "dev-main",
		"privacybydesign/irma-wp": "@dev",
		"rspeekenbrink/monolog-microsoft-teams": "^1.0",
		"vlucas/phpdotenv": "^5.0",
		"wpackagist-plugin/2fas-light": "^3.0",
		"wpackagist-plugin/coming-soon": "^6.0",
		"wpackagist-plugin/duplicate-post": "^4.0",
		"wpackagist-plugin/google-analytics-for-wordpress": "^8.0",
		"wpackagist-plugin/gravity-forms-pdf-extended": "^6.7",
		"wpackagist-plugin/onelogin-saml-sso": "^3.4",
		"wpackagist-plugin/pronamic-ideal": "^9.4",
		"wpackagist-plugin/user-switching": "^1.0",
		"wpackagist-plugin/wordpress-importer": "^0.8.1",
		"wpackagist-plugin/wp-seopress": "^6.0",
		"ypackagist/advanced-custom-fields-pro": "*",
		"ypackagist/gp-advanced-select": "^1.0.1",
		"ypackagist/gp-limit-dates": "^1.1.25",
		"ypackagist/gp-limit-submissions": "^1.1.12",
		"ypackagist/gravity-forms-power-automate": "^1.5.1",
		"ypackagist/gravityforms": "^2.8",
		"ypackagist/gravityformssurvey": "^3.8",
		"ypackagist/gravityformswebhooks": "^1.5",
		"ypackagist/gravityformszapier": "^4.2.1",
		"ypackagist/gravityperks": "^2.3.4",
		"ypackagist/gwlimitchoices": "^1.7.13",
		"ypackagist/wp-migrate-db-pro": "2.6.10"
	},
	"require-dev": {
		"10up/wp_mock": "~0.5",
		"deployer/deployer": "^6.0",
		"deployer/recipes": "dev-master",
		"friendsofphp/php-cs-fixer": "^3.14",
		"mockery/mockery": "^1.0",
		"phpunit/phpunit": "^8.0 | ^9.0",
		"symfony/var-dumper": "^5.0",
		"wpackagist-plugin/query-monitor": "*",
		"wpackagist-plugin/wa11y": "*"
	},
	"scripts": {
		"format": "vendor/bin/php-cs-fixer fix",
		"post-install-cmd": [
			"@post-root-package-install"
		],
		"post-root-package-install": [
			"php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-autoload-dump": [
			"mv htdocs/wp-content/mu-plugins/owc-shared-aura-session-instance/owc-shared-aura-session-instance.php htdocs/wp-content/mu-plugins/owc-shared-aura-session-instance.php",
			"rm -rf htdocs/wp-content/mu-plugins/owc-shared-aura-session-instance"
		],
		"test": [
			"@unit"
		],
		"unit": "./vendor/bin/phpunit --colors=always",
		"unit-coverage": "clear && XDEBUG_MODE=coverage ./vendor/bin/phpunit --stderr --colors=always --coverage-html ./tests/coverage"
	},
	"extra": {
		"wordpress-install-dir": "htdocs/wp",
		"installer-paths": {
			"htdocs/wp-content/mu-plugins/{$name}/": [
				"type:wordpress-muplugin"
			],
			"htdocs/wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"htdocs/wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		},
		"dropin-paths": {
			"htdocs/wp-content/languages/": [
				"vendor:koodimonni-language"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"HK\\": "htdocs/wp-content/themes/hollandskroon/src",
			"HW\\": "htdocs/wp-content/themes/hoekschewaard/src"
		},
		"files": []
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		},
		"classmap": []
	}
}
